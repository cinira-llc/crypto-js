{
  "name": "@cinira/crypto-js",
  "version": "1.0.0",
  "description": "Cinira shared crypto library for JavaScript",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "repository": "https://github.com/mattj65817/crypto-js.git",
  "scripts": {
    "build": "tsc -p ./tsconfig.json && tsc -p ./tsconfig-cjs.json && ./package-fixup.sh",
    "clean": "rm -rf ./dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p ./tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "test": "jest"
  },
  "keywords": [],
  "author": "Matt Jensen <mjensen65816@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "immer": "^10.0.3",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.199",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.0.3",
    "semantic-release": "^22.0.5",
    "ts-jest": "^29.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.2.2"
  },
  "release": {
    "plugins": [
      "@semantic-release/github",
      [
        "@semantic-release/npm",
        {
          "tarballDir": "./dist"
        }
      ]
    ]
  },
  "types": "./dist/esm/index.d.ts",
  "jest": {
    "preset": "ts-jest"
  }
}
